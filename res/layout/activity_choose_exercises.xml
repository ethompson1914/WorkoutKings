<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?android:attr/activatedBackgroundIndicator"
    tools:context="lin.thompson.workoutkings.Activity.ChooseExercisesActivity"
    tools:ignore="MergeRootFrame" >

    <Button
        android:id="@+id/backbutton_chooseexercises"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:text="Back" />

    <Button
        android:id="@+id/readybutton_chooseexercises"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:text="Let&apos;s Do This!" />

    <ListView
        android:id="@+id/exercise_listview1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/readybutton_chooseexercises" />

    <Button
        android:id="@+id/deletebutton_newexercise"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/backbutton_chooseexercises"
        android:text="New/Delete" />

</RelativeLayout>